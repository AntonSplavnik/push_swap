	"version": "0.2.0",
	"configurations": [
		{
			"name": "C Debug",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/a.out",
			"args": ["2345", "63457", "23457", "3457",  "463437 ", "3473 536483 3467"],
			"stopAtEntry": false,
			"cwd": "${workspaceFolder}",
			"environment": [],
			"externalConsole": false,
			"MIMode": "gdb",
			"setupCommands": [
			  {
				"description": "Enable pretty-printing for gdb",
				"text": "-enable-pretty-printing",
				"ignoreFailures": true
			  }
			],
			"miDebuggerPath": "/usr/bin/gdb", // Adjust if needed
			"preLaunchTask": "build",
			"internalConsoleOptions": "openOnSessionStart"
		  }
	]
}